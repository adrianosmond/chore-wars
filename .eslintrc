{
  "parser": "babel-eslint",
  "extends": [
    "react-app",
    "airbnb"
  ],
  "plugins": [
    "import"
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"packageDir": "./"}],
    "jsx-a11y/label-has-for": ["off"],
    "jsx-a11y/label-has-associated-control": ["off"],
    "react/jsx-filename-extension": ["off"],
    "react/prop-types": ["off"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
